{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "src",
    "bin"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "ks-react-cli": "./dist/index.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ks-react-cli-lib",
  "author": "Andrey Zhuravel",
  "module": "dist/ks-react-cli-lib.esm.js",
  "size-limit": [
    {
      "path": "dist/ks-react-cli-lib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ks-react-cli-lib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "husky": "^9.1.7",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  }
}
